{
  "name": "multi-electron",
  "version": "0.1.0",
  "private": true,
  "main": "src/main/main.js",
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "dev": "concurrently npm:start \"npx wait-on http://127.0.0.1:3000/ && npm run electron-watch\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-watch": "nodemon --exec electron .",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "electron": "^20.1.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sass": "^1.54.7",
    "tailwindcss": "^3.1.8",
    "wait-on": "^6.0.1"
  }
}
